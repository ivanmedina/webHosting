CREATE TABLE `webHosting`.`oferta` ( `id` INT NOT NULL AUTO_INCREMENT , `nombre` VARCHAR(50) NOT NULL ,
 `precio` DECIMAL NOT NULL , `espacio` DECIMAL NOT NULL , `transferencia` DECIMAL NOT NULL , `sitio` VARCHAR NOT NULL , 
 `registro` DATE NOT NULL , `cuentas` INT NOT NULL , `bases` INT NOT NULL , `subdominios` INT NOT NULL , `garantia` VARCHAR NOT NULL ,
  `panel` VARCHAR NOT NULL , `tecnologia` VARCHAR NOT NULL , `soporte` VARCHAR NOT NULL ,
   `vencimiento` VARCHAR NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;

